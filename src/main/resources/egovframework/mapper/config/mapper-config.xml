<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<!-- <setting name="mapUnderscoreToCamelCase" value="true"></setting> -->
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>
	
	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="egovMap" 			type="org.egovframe.rte.psl.dataaccess.util.EgovMap" />
		<typeAlias alias="loginEntity" 		type="egovframework.admin.login.service.domain.LoginEntity" />
		<typeAlias alias="userEntity" 		type="egovframework.admin.user.service.domain.UserEntity" />
		<typeAlias alias="user" 			type="egovframework.admin.user.service.domain.User" />
		<typeAlias alias="authEntity" 		type="egovframework.admin.user.service.domain.AuthEntity" />
		<typeAlias alias="userAuthEntity" 	type="egovframework.admin.user.service.domain.UserAuthEntity" />
		<typeAlias alias="codeGrp" 			type="egovframework.admin.mng.service.domain.CodeGrp" />
		<typeAlias alias="codeEntity" 		type="egovframework.admin.mng.service.domain.CodeEntity" />
		<typeAlias alias="code" 			type="egovframework.admin.mng.service.domain.Code" />
		<typeAlias alias="menuEntity" 		type="egovframework.admin.menu.service.domain.MenuEntity" />
		<typeAlias alias="authMenuEntity" 	type="egovframework.admin.menu.service.domain.AuthMenuEntity" />
		<typeAlias alias="authMenu" 		type="egovframework.admin.menu.service.domain.AuthMenu" />
		<typeAlias alias="boardEntity" 		type="egovframework.admin.board.service.domain.BoardEntity" />
		<typeAlias alias="postEntity" 		type="egovframework.admin.board.service.domain.PostEntity" />
		<typeAlias alias="faqEntity" 		type="egovframework.admin.board.service.domain.FaqEntity" />
		<typeAlias alias="qnaEntity" 		type="egovframework.admin.board.service.domain.QnaEntity" />
		<typeAlias alias="FileVO"			type="egovframework.com.cmm.service.FileVO" />
		<typeAlias alias="ComDefaultCodeVO" type="egovframework.com.cmm.ComDefaultCodeVO" />
		<typeAlias alias="comDefaultVO"		type="egovframework.com.cmm.ComDefaultVO" />
	</typeAliases>
    
</configuration>